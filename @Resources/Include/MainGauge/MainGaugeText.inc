[MeterCPULoadText]
Meter=String
MeasureName=MeasureCPU
MeterStyle=StyleSmallText
X=0
Y=0
Percentual=1
StringAlign=Left
FontColor=#CPUColor#
Text=CPU:%1%

[MeterRAMLoadText]
Meter=String
MeasureName=MeasureRAM
MeterStyle=StyleSmallText
StringAlign=Right
X=(#SkinWidth#-#SkinPadding#)
Y=0
Percentual=1
FontColor=#RAMColor#
Text=RAM:%1%
